{
  "comments": {
    "blockComment": [
      "[", "]"
    ]
  },
  "brackets": [
    [
      "(", ")"
    ],
    [
      "[", "]"
    ],
    [
      "{", "}"
    ],
    [
      "(+", "+)"
    ],
    [
      "(-", "-)"
    ]
  ],
  "autoClosingPairs": [
    [
      "(", ")"
    ],
    [
      "[", "]"
    ],
    [
      "{", "}"
    ],
    [
      "(+", "+)"
    ],
    [
      "(-", "-)"
    ],
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string"]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": ["string", "comment"]
    }
  ],
  "surroundingPairs": [
    [
      "(", ")"
    ],
    [
      "[", "]"
    ],
    [
      "{", "}"
    ],
    [
      "\"", "\""
    ],
    [
      "'", "'"
    ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^.*:$",
    "decreaseIndentPattern": "^\\s*[].*$"
  },
  "onEnterRules": [
    {
      "beforeText": "^\\s*$",
      "action": { "indent": "outdent" }
    }
  ]
}
