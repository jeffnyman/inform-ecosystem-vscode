{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "scopeName": "source.inform7.extension",
  "name": "Inform 7 extension",
  "patterns": [
    {
      "include": "#body"
    }
  ],
  "repository": {
    "body": {
      "begin": "(?=.)",
      "end": "$END",
      "patterns": [
        {
          "include": "source.inform7#source"
        }
      ]
    }
  }
}
